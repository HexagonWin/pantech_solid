<script type="text/javascript">
//검색
var searchFrmChk = function(){

	if($("#searchKeyWord1").val() == ""){
		alert("검색어를 입력해주세요.");
		$("#searchKeyWord1").focus();
		return false;
	}  else{		
		var search = jQuery('#searchKeyWord1').val();
		jQuery('#searchKeyWord').val(search);
		searchFrm.submit();
	}
}

var language2 = function(){
	 
		$("#ulcdForm").submit();
}  

</script>


