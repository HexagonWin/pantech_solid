	//상세 페이지
	function frmView(seq, val3, val2){
		if(!navigator.userAgent.toLowerCase().match(/msie/)){
			document.formList.val5.value = "no";
		}
		document.formList.seq.value = seq;
		document.formList.val3.value = val3;
		document.formList.val2.value = val2;
		document.formList.action = "/en/board/adTVBoardList.do;jsessionid=3093AE7BF18F147EE17B2EF0030DCEB1";
	   	document.formList.submit();
	}
	
	function mediaPlay(file_id, file_sn){
		if(navigator.userAgent.toLowerCase().match(/msie/)){
			
				$("#playerImg").hide();
				$("#MediaPlayer").show();
				document.all.mplayer.play();
				//$("#MediaPlayer").play();
			
		}else {
			alert("You can check in IE.");
			if(!navigator.userAgent.toLowerCase().match(/chrome/)){
				window.open('/cmm/fms/FileDown.do?atchFileId=' + file_id +'&fileSn=' + file_sn);
			}
		}
	}
	
	$(document).ready(function(){
		
			$("#MediaPlayer").hide();
		
	});
	
</script>	
