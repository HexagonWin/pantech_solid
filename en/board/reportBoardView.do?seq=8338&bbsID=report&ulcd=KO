<script type="text/javascript">
	function frmView(seq){
		if(seq == "" || seq == 0 || seq == null){
			alert("There are no pages.");
			return;
		}else{
		document.formView.seq.value = seq;
		document.formView.action = "/en/board/reportBoardView.do;jsessionid=C98EF4AAFC0840C7180BB287130686AA";
	   	document.formView.submit();
		}
	}
	
	function frmTopView(seq){
		document.formView.seq.value = seq;
		document.formView.sdt.value = "";
		document.formView.edt.value = "";
		document.formView.val4.value = "";
		document.formView.pageIndex.value = 1;
		document.formView.action = "/board/reportBoardView.do;jsessionid=C98EF4AAFC0840C7180BB287130686AA";
	   	document.formView.submit();
	}
	
	function frmList(){
		document.formView.action = "/en/board/reportBoardList.do;jsessionid=C98EF4AAFC0840C7180BB287130686AA";
	   	document.formView.submit();
	}
	
</script>
