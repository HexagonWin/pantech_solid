<script type="text/javascript">
var snsLink = function(sns,bitlyUrl,title){
	if(bitlyUrl == ""){bitlyUrl = location.href;}
	switch (sns) {
		case "twitter":
			/* var s_url = "http://twitter.com/share?url=" + escape(bitlyUrl) + "&text=" + encodeURIComponent(title + " ");
			window.open(s_url,"twitter","width=400,height=500,scrolls=no").focus();
			break; */
			window.open('https://twitter.com/pantech','','');
			break;
		case "facebook":
			/* var s_url = "http://www.facebook.com/share.php?u=" + escape(bitlyUrl) + "&t=" + encodeURIComponent(title);
			window.open(s_url, "facebook", "width=550,height=500,scrolls=no").focus();
			break; */
			window.open('http://www.facebook.com/vega.kr','','');
			break;
	}
};
var searchFrmChk = function(){
	if($("#id_searchKeyWord").val() == ""){
		alert("검색어를 입력해주세요.");
		$("#id_searchKeyWord").focus();
		return false;
	}else{
		searchFrm.submit();
	}
}
